{
  "name": "keywrit",
  "version": "0.0.3",
  "description": "Public key software license validator",
  "type": "module",
  "license": "MIT",
  "author": "Diego Barreiro Perez <diego@barreiro.dev>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KeyWrit/keywrit.git"
  },
  "homepage": "https://keywrit.github.io/docs/",
  "bugs": "https://github.com/KeyWrit/keywrit/issues",
  "keywords": ["license", "validation", "ed25519", "jwt"],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.global.js",
  "jsdelivr": "dist/index.global.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "prepublishOnly": "bun run build",
    "build": "bunup",
    "dev": "bunup --watch",
    "test": "bun test",
    "coverage": "bun test --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@noble/ed25519": "^3.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "bunup": "^0.16.22"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  }
}
